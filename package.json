{
  "name": "git-tree",
  "version": "0.1.0",
  "private": true,
  "main": "src/start.js",
  "build": {
    "appId": "git-tree",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "productivity",
      "iconUrl": "./src/assets/images/git-branch.svg"
    },
    "mac": {
      "category": "productivity",
      "iconUrl": "./src/assets/images/git-branch.svg"
    }
  },
  "scripts": {
    "electron2": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "rebuild": "electron-rebuild --runtime=electron --target=8.2.0 --disturl=https://atom.io/download/electron --build-from-source",
    "electron": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "cross-env BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^3.24.0",
    "@blueprintjs/timezone": "^3.6.1",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.13",
    "@types/nodegit": "^0.26.2",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "electron": "8.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-rebuild": "^1.10.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "nodegit": "^0.26.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.4",
    "react-scripts": "3.4.1",
    "react-split-pane": "^0.1.91",
    "redux-zero": "^5.1.3",
    "redux-zero-persist": "^0.2.6"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.3.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.4.1",
    "electron-reload": "^1.5.0",
    "file-loader": "^6.0.0",
    "nodemon": "^2.0.2",
    "react-app-rewired": "^2.1.5",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.1"
  }
}
